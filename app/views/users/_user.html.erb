<%= form_for user do |f| %>
	<%= f.label	:name %>
	<%= f.text_field :name %> <br>
	<%= f.label :password %>
	<%= f.password_field :password %> <br>
	<%= f.label :nausea %><br>
	<%= f.number_field :nausea %><br>
	<%= f.label :happiness %><br>
	<%= f.number_field :happiness %><br>
	<%= f.label :tickets %><br>
	<%= f.number_field :tickets %><br>
	<%= f.label :height %><br>
	<%= f.number_field :height %><br>

  <%= f.label :admin%>
  <%= f.check_box :admin%>
  
	<%= f.submit %>
<% end %>

